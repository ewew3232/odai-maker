ateItem(updatedItem) {\n      const index = this.items.findIndex(item => item.id === updatedItem.id);\n      if (index !== -1) {\n        this.items.splice(index, 1, updatedItem);\n        localStorage.setItem('items', JSON.stringify(this.items));\n      }\n    }\n  },\n  created() {\n    const savedItems = localStorage.getItem('items');\n    if (savedItems) {\n      this.items = JSON.parse(savedItems);\n    }\n  }\n}\n</script>\n\n\n\n<style>\n/* ここに共通のCSSを書く */\n</style>\n"],"mappings":";AAeA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,IAAG,CAAG;IACrB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAACC,OAAO,EAAE;MACf,MAAMC,IAAG,GAAI;QAAEC,EAAE,EAAEC,IAAI,CAACC,GAAG,CAAC,CAAC;QAAEV,IAAI,EAAEM,OAAO,CAACN,IAAI;QAAEW,KAAK,EAAEL,OAAO,CAACK;MAAM,CAAC;MACzE,IAAI,CAACT,KAAK,CAACU,IAAI,CAACL,IAAI,CAAC;MACrBM,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACd,KAAK,CAAC,CAAC;IAC3D,CAAC;IACDe,UAAUA,CAACC,WAAW,EAAE;MACtB,MAAMC,KAAI,GAAI,IAAI,CAACjB,KAAK,CAACkB,SAAS,CAACb,IAAG,IAAKA,IAAI,CAACC,EAAC,KAAMU,WAAW,CAACV,EAAE,CAAC;MACtE,IAAIW,KAAI,KAAM,CAAC,CAAC,EAAE;QAChB,IAAI,CAACjB,KAAK,CAACmB,MAAM,CAACF,KAAK,EAAE,CAAC,EAAED,WAAW,CAAC;QACxCL,YAAY,CAACC,OAAO,CAAC,OAAO,EAAEC,IAAI,CAACC,SAAS,CAAC,IAAI,CAACd,KAAK,CAAC,CAAC;MAC3D;IACF;EACF,CAAC;EACDoB,OAAOA,CAAA,EAAG;IACR,MAAMC,UAAS,GAAIV,YAAY,CAACW,OAAO,CAAC,OAAO,CAAC;IAChD,IAAID,UAAU,EAAE;MACd,IAAI,CAACrB,KAAI,GAAIa,IAAI,CAACU,KAAK,CAACF,UAAU,CAAC;IACrC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}